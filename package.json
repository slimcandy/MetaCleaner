  "repository": {
    "type": "git",
    "url": "git+https://github.com/slimcandy/removemetaonline.git"
  },
  "bugs": {
    "url": "https://github.com/slimcandy/removemetaonline/issues"
  },
  "homepage": "https://metacleaner.js.garden"
}
